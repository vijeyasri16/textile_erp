generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id              Int     @id @default(autoincrement())
  name            String
  phone           String
  deliveryAddress String
}

model Employee {
  id       Int     @id @default(autoincrement())
  name     String
  password String
}

model Fabric {
  id          Int     @id @default(autoincrement())
  name        String
  composition String
}

model Machine {
  id           Int     @id @default(autoincrement())
  name         String
  processLinked String
}

model Supplier {
  id      Int     @id @default(autoincrement())
  name    String
  contact String
  address String
}
